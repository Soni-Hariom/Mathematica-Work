Cell[BoxData[{
 RowBox[{
  RowBox[{"newton", "[", 
   RowBox[{"f_", ",", "x0_", ",", "Nmax_", ",", "eps_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"y0", ",", "x", ",", "x1", ",", "x0p"}], "}"}], ",", 
    RowBox[{
     RowBox[{"y0", "=", "x0"}], ";", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<f(x) := \>\"", ",", 
       RowBox[{"f", "[", "x", "]"}]}], "]"}], ";", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<f'(x):= \>\"", ",", 
       RowBox[{
        RowBox[{"f", "'"}], "[", "x", "]"}]}], "]"}], ";", 
     RowBox[{"For", "[", " ", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", " ", 
       RowBox[{"i", "<=", " ", "Nmax"}], ",", 
       RowBox[{"i", "++"}], ",", " ", 
       RowBox[{
        RowBox[{"x1", "=", 
         RowBox[{"N", "[", 
          RowBox[{"x0", "-", 
           RowBox[{
            RowBox[{"f", "[", "x0", "]"}], "/", 
            RowBox[{
             RowBox[{"f", "'"}], "[", "x0", "]"}]}]}], "]"}]}], ";", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Abs", "[", 
            RowBox[{"x1", "-", "x0"}], "]"}], "<", "eps"}], ",", 
          RowBox[{"Return", "[", "x1", "]"}], ",", 
          RowBox[{
           RowBox[{"x0p", "=", "x0"}], ";", 
           RowBox[{"x0", "=", "x1"}]}]}], "]"}], ";", 
        RowBox[{"Print", "[", 
         RowBox[{
         "\"\<In \>\"", ",", "i", ",", 
          "\"\<th Number of iterations the approximation to root is : \>\"", 
          ",", "x1"}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"Print", "[", 
         RowBox[{"\"\< Estimated error is : \>\"", ",", 
          RowBox[{"Abs", "[", 
           RowBox[{"x1", "-", "x0p"}], "]"}]}], "]"}]}]}], "]"}], ";", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<the final approximation of root is : \>\"", ",", "x1"}], 
      "]"}], ";", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\< Estimated error is : \>\"", ",", 
       RowBox[{"Abs", "[", 
        RowBox[{"x1", "-", "x0"}], "]"}]}], "]"}], ";", 
     RowBox[{"Plot", "[", 
      RowBox[{
       RowBox[{"f", "[", "x", "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", 
         RowBox[{"-", "1"}], ",", "3"}], "}"}]}], "]"}]}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x", "]"}], ":=", 
   RowBox[{"Cos", "[", "x", "]"}]}], ";"}], "\[IndentingNewLine]", 
